<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.openbyodv01"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.openbyodv01.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.openbyodv01.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible" >
        </activity>
        <activity android:name="com.example.openbyodv01.Launcher" >
        </activity>
        <activity
            android:name="com.example.openbyodv01.Agenda"
            android:label="@string/title_activity_agenda" >
        </activity>
        <activity
            android:name="com.example.openbyodv01.Inbox"
            android:label="@string/title_activity_inbox" >
        </activity>
        <activity
            android:name="com.example.openbyodv01.ReadMail"
            android:label="@string/title_activity_read_mail" >
        </activity>
        <activity
            android:name="com.example.openbyodv01.Compose"
            android:label="@string/title_activity_compose" >
        </activity>
        <activity
            android:name="com.example.openbyodv01.NewContact"
            android:label="@string/title_activity_new_contact" >
        </activity>
    </application>

</manifest>
